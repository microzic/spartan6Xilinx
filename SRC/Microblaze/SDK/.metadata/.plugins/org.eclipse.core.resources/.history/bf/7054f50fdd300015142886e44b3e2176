/*smg display test */

#include <stdio.h>
#include "platform.h"
#include "xparameters.h"
#include "smg_ip.h"

void print(char *str);

int main()
{
    init_platform();

    SMG_IP_mReset(XPAR_SMG_IP_0_BASEADDR);	// Reset USB IP

	while (1)
	{
		SMG_IP_mWriteSlaveReg0(XPAR_SMG_IP_0_BASEADDR,0,smg_data);
	}


    return 0;
}
